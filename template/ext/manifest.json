{
  "name": "$(PROJECT_NAME)",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    },
    "default_title": "Popup Page",
    "default_popup": "pages/popup/index.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab_override.html"
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "version": "0.0.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}