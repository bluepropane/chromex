{
  "private": true,
  "name": "chromex",
  "version": "1.0.0",
  "description": "Create chrome extensions in a minute",
  "workspaces": [
    "packages/*"
  ],
  "repository": "https://github.io/bluepropane",
  "author": "bluepropane <rbluepropane@gmail.com>",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
