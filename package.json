{
  "name": "create-chrome-extension",
  "version": "1.0.0",
  "description": "Chrome extensions in a minute",
  "main": "index.js",
  "repository": "https://github.io",
  "author": "bluepropane <yoyozwassup@hotmail.com>",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
    "chrome",
    "boilerplate"
  ],
  "scripts": {
    "dev": "NODE_ENV=development webpack-dashboard -- webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "commit": "git-cz",
    "release": "standard-version",
    "pack": "yarn build && bestzip ext.zip ext/",
    "genss": "node scripts/genss.js"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^6.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "puppeteer-core": "^1.18.1",
    "standard-version": "^6.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "ncp": "^2.0.0"
  }
}
